ADMIN_TOKEN=<32-hex>
OPENCLAW_DOMAIN=openclaw.example.com
CLOUDFLARE_API_TOKEN=<token>
CLOUDFLARE_ZONE_ID=<zone-id>
GATEWAY_DOMAIN=_.your-gateway.example.com
CERTBOT_EMAIL=admin@example.com
OPENCLAW_IMAGE=openclaw-nearai-worker:local

# Updater configuration
COMPOSE_API_IMAGE=openclaw-compose-api:local
COMPOSE_API_IMAGE_REPO=docker.io/youruser/openclaw-compose-api
WORKER_IMAGE_REPO=docker.io/youruser/openclaw-nearai-worker
UPDATER_IMAGE=openclaw-updater:local
UPDATER_IMAGE_REPO=docker.io/youruser/openclaw-updater
UPDATER_CHANNEL=latest
UPDATER_POLL_INTERVAL=300
UPDATER_COSIGN_IDENTITY=https://github.com/yourorg/yourrepo/.github/workflows/.*
DEPLOY_DIR=/home/root/openclaw/deploy

# ── dstack CVM production overrides ──────────────────────────────────
# Uncomment these when deploying via dstack (no SSH access).
# The updater uses a base env (read-only, regenerated on boot) plus a
# persistent overrides file on a Docker volume for pinned image digests.
# DEPLOY_DIR=/dstack
# UPDATER_COMPOSE_FILE=/app/deploy/docker-compose.yaml
# UPDATER_ENV_FILE=/app/data/.env.overrides
# UPDATER_BASE_ENV_FILE=/app/deploy/.host-shared/.decrypted-env
